{
    "name": "lionser/cidr",
    "type": "library",
    "description": "Library for parse CIDR from ip range.",
    "keywords": [
        "ip",
        "ip range",
        "cidr",
        "parser"
    ],
    "homepage": "https://github.com/lionser/cidr",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Levchuk",
            "email": "dev.lionser@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.5",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Lionser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lionser\\Tests\\": "tests/"
        }
    }
}
